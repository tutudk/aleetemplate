
@font-face {
    font-family: 'quentin';
    src:url('{{ 'quentin.eot' | asset_url }}');
    src:url('{{ 'quentin.eot' | asset_url }}') format('embedded-opentype'),
        url('{{ 'quentin.woff' | asset_url }}') format('woff'),
        url('{{ 'quentin.woff2' | asset_url }}') format('woff2'),
        url('{{ 'quentin.ttf' | asset_url }}') format('truetype'),
        url('{{ 'quentin.svg' | asset_url }}') format('svg');
    font-weight: 400;
    font-style: normal;
}